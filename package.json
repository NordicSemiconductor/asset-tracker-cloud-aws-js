{
  "name": "@nordicsemiconductor/asset-tracker-cloud-aws",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using AWS CDK in TypeScript.",
  "main": "./dist/export.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-apigatewayv2": "3.9.0",
    "@aws-sdk/client-cloudformation": "3.9.0",
    "@aws-sdk/client-cloudwatch-logs": "3.9.0",
    "@aws-sdk/client-codepipeline": "3.9.0",
    "@aws-sdk/client-dynamodb": "3.9.0",
    "@aws-sdk/client-iot": "3.9.0",
    "@aws-sdk/client-s3": "3.8.1",
    "@aws-sdk/client-sqs": "3.8.1",
    "@aws-sdk/client-ssm": "3.8.1",
    "@aws-sdk/client-sts": "3.8.1",
    "@aws-sdk/client-timestream-query": "3.8.1",
    "@aws-sdk/client-timestream-write": "3.8.1",
    "@nordicsemiconductor/cell-geolocation-helpers": "3.0.1",
    "@nordicsemiconductor/cloudformation-helpers": "6.0.11",
    "@nordicsemiconductor/e2e-bdd-test-runner": "13.0.4",
    "@nordicsemiconductor/random-words": "5.0.0",
    "@nordicsemiconductor/timestream-helpers": "3.1.3",
    "@sinclair/typebox": "0.15.0",
    "ajv": "7.2.1",
    "fp-ts": "2.9.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.94.1",
    "@aws-cdk/aws-cloudfront": "1.94.1",
    "@aws-cdk/aws-codebuild": "1.94.1",
    "@aws-cdk/aws-codepipeline": "1.94.1",
    "@aws-cdk/aws-cognito": "1.94.1",
    "@aws-cdk/aws-dynamodb": "1.94.1",
    "@aws-cdk/aws-events-targets": "1.94.1",
    "@aws-cdk/aws-iam": "1.94.1",
    "@aws-cdk/aws-iot": "1.94.1",
    "@aws-cdk/aws-s3": "1.94.1",
    "@aws-cdk/aws-ssm": "1.94.1",
    "@aws-cdk/aws-stepfunctions": "1.94.1",
    "@aws-cdk/aws-stepfunctions-tasks": "1.94.1",
    "@aws-cdk/aws-timestream": "1.94.1",
    "@aws-cdk/core": "1.94.1",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "12.0.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.12",
    "@nordicsemiconductor/firmware-ci-device-helpers": "6.1.2",
    "@nordicsemiconductor/object-to-env": "2.0.1",
    "@nordicsemiconductor/package-layered-lambdas": "6.2.20",
    "@octokit/rest": "18.3.5",
    "@types/aws-iot-device-sdk": "2.2.2",
    "@types/aws-lambda": "8.10.72",
    "@types/backoff": "2.5.1",
    "@types/cfn-response": "1.0.4",
    "@types/chai-subset": "1.3.3",
    "@types/chalk": "2.2.0",
    "@types/commander": "2.12.2",
    "@types/jest": "26.0.21",
    "@types/node": "14.14.35",
    "@types/uuid": "8.3.0",
    "aws-iot-device-sdk": "2.2.6",
    "backoff": "2.5.0",
    "cdk": "1.94.1",
    "chalk": "4.1.0",
    "change-case": "4.1.2",
    "commander": "7.1.0",
    "date-fns": "2.19.0",
    "eslint": "7.22.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "nock": "13.0.11",
    "octokit-auth-netrc": "1.0.0",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/util",
    "dist/feature-runner",
    "dist/cli",
    "cli/index.js",
    "LICENSE",
    "README.md"
  ],
  "deploy": {
    "branches": [
      "saga"
    ],
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git",
      "branch": "saga"
    },
    "deviceUI": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-js.git",
      "branch": "saga"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
