{
  "name": "@nordicsemiconductor/asset-tracker-cloud-aws",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using AWS CDK in TypeScript.",
  "types": "./export.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "find ./ -type f -name '*.spec.ts' -print | grep -v /node_modules/ | grep -v /dist/ | xargs npx tsx --no-warnings --test",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky install && check-node-version --package",
    "prepublishOnly": "npx swc -d dist ./cli npx swc -d dist ./features"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-docs": "36.0.0",
    "@nordicsemiconductor/device-helpers": "17.0.40",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "6.0.0",
    "@nordicsemiconductor/timestream-helpers": "6.0.1",
    "@sinclair/typebox": "0.31.28",
    "ajv": "8.12.0",
    "jsonwebtoken": "9.0.2"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewayv2": "3.489.0",
    "@aws-sdk/client-cloudformation": "3.489.0",
    "@aws-sdk/client-cloudwatch-logs": "3.489.0",
    "@aws-sdk/client-codepipeline": "3.489.0",
    "@aws-sdk/client-cognito-identity": "3.489.0",
    "@aws-sdk/client-cognito-identity-provider": "3.489.0",
    "@aws-sdk/client-dynamodb": "3.489.0",
    "@aws-sdk/client-iot": "3.489.0",
    "@aws-sdk/client-iot-data-plane": "3.489.0",
    "@aws-sdk/client-s3": "3.489.0",
    "@aws-sdk/client-sfn": "3.489.0",
    "@aws-sdk/client-sqs": "3.489.0",
    "@aws-sdk/client-ssm": "3.489.0",
    "@aws-sdk/client-sts": "3.489.0",
    "@aws-sdk/client-timestream-query": "3.489.0",
    "@aws-sdk/client-timestream-write": "3.489.0",
    "@aws-sdk/util-dynamodb": "3.489.0",
    "@commitlint/config-conventional": "18.4.4",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.6",
    "@nordicsemiconductor/bdd-markdown": "7.0.15",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@nordicsemiconductor/object-to-env": "7.0.5",
    "@nordicsemiconductor/random-words": "8.0.0",
    "@octokit/rest": "20.0.2",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.102",
    "@types/aws-lambda": "8.10.131",
    "@types/backoff": "2.5.5",
    "@types/cfn-response": "1.0.8",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "20.10.8",
    "@types/yazl": "2.4.5",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "aws-cdk-lib": "2.118.0",
    "aws-iot-device-sdk-v2": "1.19.0",
    "backoff": "2.5.0",
    "cdk": "2.118.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "11.1.0",
    "constructs": "10.3.0",
    "date-fns": "3.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "50.0.1",
    "glob": "10.3.10",
    "tsmatchers": "^5.0.2",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/util",
    "dist/feature-runner",
    "dist/cli",
    "cli/index.js",
    "LICENSE",
    "README.md"
  ],
  "deploy": {
    "branch": "saga",
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git",
      "branch": "saga"
    }
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
