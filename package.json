{
  "name": "@nordicsemiconductor/asset-tracker-cloud-aws",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using AWS CDK in TypeScript.",
  "main": "./dist/export.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "husky install && check-node-version --package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.45.0",
    "@aws-sdk/client-iot-data-plane": "3.45.0",
    "@aws-sdk/client-sfn": "3.45.0",
    "@aws-sdk/client-sqs": "3.45.0",
    "@aws-sdk/client-ssm": "3.45.0",
    "@aws-sdk/client-timestream-write": "3.45.0",
    "@aws-sdk/util-dynamodb": "3.45.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "3.1.6",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "1.2.1",
    "@nordicsemiconductor/timestream-helpers": "3.2.20",
    "@sinclair/typebox": "0.23.2",
    "ajv": "8.8.2",
    "fp-ts": "2.11.5",
    "jsonwebtoken": "8.5.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewayv2": "3.45.0",
    "@aws-sdk/client-cloudformation": "3.45.0",
    "@aws-sdk/client-cloudwatch-logs": "3.45.0",
    "@aws-sdk/client-codepipeline": "3.45.0",
    "@aws-sdk/client-iot": "3.45.0",
    "@aws-sdk/client-s3": "3.45.0",
    "@aws-sdk/client-sts": "3.45.0",
    "@aws-sdk/client-timestream-query": "3.45.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "10.0.7",
    "@nordicsemiconductor/cloudformation-helpers": "6.1.25",
    "@nordicsemiconductor/e2e-bdd-test-runner": "14.4.108",
    "@nordicsemiconductor/firmware-ci-device-helpers": "12.0.14",
    "@nordicsemiconductor/object-to-env": "2.1.2",
    "@nordicsemiconductor/package-layered-lambdas": "9.0.62",
    "@nordicsemiconductor/random-words": "5.1.2",
    "@octokit/rest": "18.12.0",
    "@types/aws-iot-device-sdk": "2.2.4",
    "@types/aws-lambda": "8.10.89",
    "@types/backoff": "2.5.2",
    "@types/cfn-response": "1.0.5",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "27.4.0",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "17.0.1",
    "@types/uuid": "8.3.3",
    "aws-cdk-lib": "2.3.0",
    "aws-iot-device-sdk": "2.2.11",
    "backoff": "2.5.0",
    "cdk": "2.3.0",
    "chalk": "4.1.2",
    "change-case": "4.1.2",
    "check-node-version": "4.2.1",
    "commander": "8.3.0",
    "constructs": "10.0.23",
    "date-fns": "2.28.0",
    "jest": "27.4.5",
    "nock": "13.2.1",
    "octokit-auth-netrc": "1.0.1",
    "ts-jest": "27.1.2"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/util",
    "dist/feature-runner",
    "dist/cli",
    "cli/index.js",
    "LICENSE",
    "README.md"
  ],
  "deploy": {
    "branches": [
      "saga"
    ],
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git",
      "branch": "saga"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  }
}
