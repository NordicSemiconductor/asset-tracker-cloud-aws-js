{
  "name": "@nordicsemiconductor/asset-tracker-cloud-aws",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using AWS CDK in TypeScript.",
  "main": "./dist/export.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "husky install && check-node-version --package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.105.0",
    "@aws-sdk/client-iot-data-plane": "3.105.0",
    "@aws-sdk/client-sfn": "3.105.0",
    "@aws-sdk/client-sqs": "3.107.0",
    "@aws-sdk/client-ssm": "3.105.0",
    "@aws-sdk/client-timestream-write": "3.105.0",
    "@aws-sdk/util-dynamodb": "3.105.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "5.0.1",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "2.0.7",
    "@nordicsemiconductor/timestream-helpers": "4.1.2",
    "@sinclair/typebox": "0.23.5",
    "ajv": "8.11.0",
    "fp-ts": "2.12.1",
    "jsonwebtoken": "8.5.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewayv2": "3.105.0",
    "@aws-sdk/client-cloudformation": "3.107.0",
    "@aws-sdk/client-cloudwatch-logs": "3.105.0",
    "@aws-sdk/client-codepipeline": "3.105.0",
    "@aws-sdk/client-iot": "3.105.0",
    "@aws-sdk/client-s3": "3.107.0",
    "@aws-sdk/client-sts": "3.105.0",
    "@aws-sdk/client-timestream-query": "3.105.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.83",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.6",
    "@nordicsemiconductor/e2e-bdd-test-runner": "15.1.19",
    "@nordicsemiconductor/firmware-ci-device-helpers": "13.0.161",
    "@nordicsemiconductor/object-to-env": "4.1.1",
    "@nordicsemiconductor/package-layered-lambdas": "11.0.56",
    "@nordicsemiconductor/random-words": "6.0.0",
    "@octokit/rest": "18.12.0",
    "@swc/jest": "0.2.21",
    "@types/aws-iot-device-sdk": "2.2.4",
    "@types/aws-lambda": "8.10.100",
    "@types/backoff": "2.5.2",
    "@types/cfn-response": "1.0.5",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "28.1.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.41",
    "@types/uuid": "8.3.4",
    "aws-cdk-lib": "2.27.0",
    "aws-iot-device-sdk": "2.2.12",
    "backoff": "2.5.0",
    "cdk": "2.27.0",
    "chalk": "4.1.2",
    "change-case": "4.1.2",
    "check-node-version": "4.2.1",
    "commander": "9.3.0",
    "constructs": "10.1.36",
    "date-fns": "2.28.0",
    "jest": "28.1.1",
    "nock": "13.2.6",
    "octokit-auth-netrc": "2.0.0"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/util",
    "dist/feature-runner",
    "dist/cli",
    "cli/index.js",
    "LICENSE",
    "README.md"
  ],
  "deploy": {
    "branches": [
      "saga"
    ],
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git",
      "branch": "saga"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
