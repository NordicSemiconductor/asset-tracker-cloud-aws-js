{
  "name": "@bifravst/aws",
  "version": "0.0.0-development",
  "description": "AWS-based implementation of Bifravst",
  "scripts": {
    "test": "jest",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress",
    "postinstall": "check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/aws.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/aws/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-dynamodb-v2-node": "0.1.0-preview.2",
    "@bifravst/athena-helpers": "8.0.58",
    "@bifravst/cell-geolocation-helpers": "1.3.14",
    "@bifravst/device-ui-server": "2.0.1",
    "@bifravst/random-words": "4.2.1",
    "ajv": "6.12.5",
    "aws-iot-device-sdk": "2.2.6",
    "aws-sdk": "2.761.0",
    "backoff": "2.5.0",
    "chalk": "4.1.0",
    "commander": "6.1.0",
    "date-fns": "2.16.1",
    "fp-ts": "2.8.3",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.64.0",
    "@aws-cdk/aws-athena": "1.64.0",
    "@aws-cdk/aws-cloudfront": "1.64.0",
    "@aws-cdk/aws-codebuild": "1.64.0",
    "@aws-cdk/aws-codepipeline": "1.64.0",
    "@aws-cdk/aws-cognito": "1.64.0",
    "@aws-cdk/aws-dynamodb": "1.64.0",
    "@aws-cdk/aws-events-targets": "1.64.0",
    "@aws-cdk/aws-iam": "1.64.0",
    "@aws-cdk/aws-iot": "1.64.0",
    "@aws-cdk/aws-s3": "1.64.0",
    "@aws-cdk/aws-ssm": "1.64.0",
    "@aws-cdk/aws-stepfunctions": "1.64.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.64.0",
    "@aws-cdk/core": "1.64.0",
    "@bifravst/cloudformation-helpers": "3.1.43",
    "@bifravst/code-style": "8.0.94",
    "@bifravst/e2e-bdd-test-runner": "2.6.13",
    "@bifravst/package-layered-lambdas": "3.10.36",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/aws-iot-device-sdk": "2.2.2",
    "@types/aws-lambda": "8.10.62",
    "@types/backoff": "2.5.1",
    "@types/cfn-response": "1.0.3",
    "@types/chai-subset": "1.3.3",
    "@types/chalk": "2.2.0",
    "@types/commander": "2.12.2",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.2",
    "@types/uuid": "8.3.0",
    "cdk": "1.64.0",
    "change-case": "4.1.1",
    "check-node-version": "4.0.3",
    "compare-versions": "3.6.0",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "nock": "13.0.4",
    "ts-jest": "26.4.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npx tsc && npx jest --onlyChanged"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "deploy": {
    "branches": [
      "saga"
    ],
    "webApp": {
      "repository": "https://github.com/bifravst/app.git",
      "branch": "saga"
    },
    "deviceUI": {
      "repository": "https://github.com/bifravst/device-ui.git",
      "branch": "saga"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
