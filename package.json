{
  "name": "@nordicsemiconductor/asset-tracker-cloud-aws",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using AWS CDK in TypeScript.",
  "main": "./dist/export.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:e2e": "node --unhandled-rejections=strict --loader tsx feature-runner/run-features.ts ./features --print-results --progress",
    "prepare": "husky install && check-node-version --package",
    "prepublishOnly": "npx tsc --noEmit false --outDir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.210.0",
    "@aws-sdk/client-iot-data-plane": "3.210.0",
    "@aws-sdk/client-sfn": "3.210.0",
    "@aws-sdk/client-sqs": "3.210.0",
    "@aws-sdk/client-ssm": "3.210.0",
    "@aws-sdk/client-timestream-write": "3.210.0",
    "@aws-sdk/util-dynamodb": "3.210.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "5.0.2",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "3.0.1",
    "@nordicsemiconductor/timestream-helpers": "4.1.3",
    "@sinclair/typebox": "0.25.8",
    "ajv": "8.11.2",
    "entities": "4.4.0",
    "fast-xml-parser": "4.0.11",
    "fp-ts": "2.13.1",
    "jsonwebtoken": "8.5.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewayv2": "3.210.0",
    "@aws-sdk/client-cloudformation": "3.210.0",
    "@aws-sdk/client-cloudwatch-logs": "3.210.0",
    "@aws-sdk/client-codepipeline": "3.210.0",
    "@aws-sdk/client-iot": "3.210.0",
    "@aws-sdk/client-s3": "3.210.0",
    "@aws-sdk/client-sts": "3.210.0",
    "@aws-sdk/client-timestream-query": "3.210.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.140",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.7",
    "@nordicsemiconductor/e2e-bdd-test-runner": "15.1.158",
    "@nordicsemiconductor/firmware-ci-device-helpers": "13.2.33",
    "@nordicsemiconductor/object-to-env": "4.1.2",
    "@nordicsemiconductor/package-layered-lambdas": "11.0.121",
    "@nordicsemiconductor/random-words": "6.0.1",
    "@octokit/rest": "19.0.5",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.16",
    "@swc/jest": "0.2.23",
    "@types/aws-iot-device-sdk": "2.2.4",
    "@types/aws-lambda": "8.10.108",
    "@types/backoff": "2.5.2",
    "@types/cfn-response": "1.0.5",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "29.2.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.9",
    "@types/uuid": "8.3.4",
    "aws-cdk-lib": "2.50.0",
    "aws-iot-device-sdk": "2.2.12",
    "backoff": "2.5.0",
    "cdk": "2.50.0",
    "chalk": "4.1.2",
    "change-case": "4.1.2",
    "check-node-version": "4.2.1",
    "commander": "9.4.1",
    "constructs": "10.1.159",
    "date-fns": "2.29.3",
    "jest": "29.3.1",
    "nock": "13.2.9",
    "octokit-auth-netrc": "2.0.1",
    "tsx": "3.12.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/util",
    "dist/feature-runner",
    "dist/cli",
    "cli/index.js",
    "LICENSE",
    "README.md"
  ],
  "deploy": {
    "branches": [
      "saga"
    ],
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git",
      "branch": "saga"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
