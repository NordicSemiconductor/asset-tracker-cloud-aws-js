{
  "name": "@nordicsemiconductor/asset-tracker-cloud-aws",
  "version": "0.0.0-development",
  "description": "A reference implementation of a serverless backend for an IoT product developed using AWS CDK in TypeScript.",
  "types": "./export.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky install && check-node-version --package",
    "prepublishOnly": "npx swc -d dist ./cli npx swc -d dist ./features"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-docs": "31.0.2",
    "@nordicsemiconductor/device-helpers": "15.0.20",
    "@nordicsemiconductor/nrfcloud-location-services-tests": "4.0.3",
    "@nordicsemiconductor/timestream-helpers": "5.0.0",
    "@sinclair/typebox": "0.31.15",
    "ajv": "8.12.0",
    "jsonwebtoken": "9.0.2"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewayv2": "3.421.0",
    "@aws-sdk/client-cloudformation": "3.421.0",
    "@aws-sdk/client-cloudwatch-logs": "3.421.0",
    "@aws-sdk/client-codepipeline": "3.421.0",
    "@aws-sdk/client-cognito-identity": "3.421.0",
    "@aws-sdk/client-cognito-identity-provider": "3.421.0",
    "@aws-sdk/client-dynamodb": "3.421.0",
    "@aws-sdk/client-iot": "3.421.0",
    "@aws-sdk/client-iot-data-plane": "3.421.0",
    "@aws-sdk/client-s3": "3.421.0",
    "@aws-sdk/client-sfn": "3.421.0",
    "@aws-sdk/client-sqs": "3.421.0",
    "@aws-sdk/client-ssm": "3.421.0",
    "@aws-sdk/client-sts": "3.421.0",
    "@aws-sdk/client-timestream-query": "3.421.0",
    "@aws-sdk/client-timestream-write": "3.421.0",
    "@aws-sdk/util-dynamodb": "3.421.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.93",
    "@nordicsemiconductor/bdd-markdown": "7.0.0",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/object-to-env": "5.0.0",
    "@nordicsemiconductor/random-words": "7.0.0",
    "@octokit/rest": "20.0.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.90",
    "@swc/jest": "0.2.29",
    "@types/aws-lambda": "8.10.122",
    "@types/backoff": "2.5.3",
    "@types/cfn-response": "1.0.6",
    "@types/jest": "29.5.5",
    "@types/jsonwebtoken": "9.0.3",
    "@types/node": "20.8.0",
    "@types/yazl": "2.4.3",
    "aws-cdk-lib": "2.99.1",
    "aws-iot-device-sdk-v2": "1.16.0",
    "backoff": "2.5.0",
    "cdk": "2.99.1",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "11.0.0",
    "constructs": "10.2.70",
    "date-fns": "2.30.0",
    "glob": "10.3.10",
    "jest": "29.7.0",
    "tsmatchers": "5.0.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x|*_*|*/*)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/export.js",
    "dist/export.d.ts",
    "dist/util",
    "dist/feature-runner",
    "dist/cli",
    "cli/index.js",
    "LICENSE",
    "README.md"
  ],
  "deploy": {
    "branch": "saga",
    "webApp": {
      "repository": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git",
      "branch": "saga"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
